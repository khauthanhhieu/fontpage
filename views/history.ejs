<!DOCTYPE html>
<html lang="vi">

<head>
    <title>Lịch sử giao dịch | E-Shopping</title>
    <% include partials/head %>
</head>
<!--/head-->

<body>
    <% include partials/header %>
    <section id="cart_items">
        <div class="container">
            <div class="breadcrumbs">
                <ol class="breadcrumb">
                    <li><a href="/">Home</a></li>
                    <li class="active">Lịch sử giao dịch</li>
                </ol>
            </div>
            <div class="table-responsive cart_info">
                <table class="table table-condensed">
                    <thead>
                        <tr class="cart_menu">
                            <td class="description">Tên sản phẩm</td>
                            <td class="quantity">Số lượng</td>
                            <td class="total">Thành tiền</td>
                            <td class="date">Ngày đặt hàng</td>
                            <td class="status">Trạng thái</td>
                            <td class="status"></td>
                            <td></td>
                        </tr>
                    </thead>
                    <tbody>
                        <% oList.forEach(function(oItem){ %>
                        <tr>
                            <td class="cart_description">
                                <h4><a href=""><%= oItem.name %></a></h4>
                                <!-- <p>Web ID: 1089772</p> -->
                            </td>
                            <td class="cart_quantity">
                                <h4><%= oItem.number %></h4>
                            </td>
                            <td class="cart_total">
                                <p class="cart_total_price"><%= oItem.price %></p>
                            </td>
                            <td class="order_date">
                                <p><%= oItem.date %></p>
                            </td>
                            <td class="item_stt">
                                <h5><%= oItem.Status %></h5>
                            </td>
                           <!--  <td>
                                <form action="" method="post" >
                                    <button type="submit" class="btn" visible="">Xóa</button>
                                </form>
                            </td> -->
                        </tr>
                        <% }); %>
                    </tbody>
                </table>
            </div>
        </div>
    </section>
    <% include partials/footer %>
    <script src="/javascripts/jquery.js"></script>
    <script src="/javascripts/bootstrap.min.js"></script>
    <script src="/javascripts/jquery.scrollUp.min.js"></script>
    <script src="/javascripts/jquery.prettyPhoto.js"></script>
    <script src="/javascripts/main.js"></script>
    <script src="/javascripts/client.js"></script>
</body>

</html>